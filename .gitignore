# Cache, session files and logs (Symfony6)
/app/var/cache/*
/app/var/logs/*
/app/var/sessions/*
!app/var/cache/.gitkeep
!app/var/logs/.gitkeep
!app/var/sessions/.gitkeep

# Parameters
/app/config/parameters.yml
/app/config/parameters.ini

# Managed by Composer
/app/bootstrap.php.cache
/app/var/bootstrap.php.cache
/app/bin/*
!app/bin/console
!app/bin/symfony_requirements
/app/vendor/

# PHPUnit
/app/phpunit.xml

# Backup entities generated with doctrine:generate:entities command
**/Entity/*~

# Embedded web-server pid file
/.web-server-pid
